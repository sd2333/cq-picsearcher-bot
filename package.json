  "main": "index.mjs",
  "private": true,
  "scripts": {
    "test": "node index.mjs",
    "dev": "nodemon index.mjs --ignore data --exec \"node --inspect\"",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "log": "pm2 logs cqps",
    "kill": "pm2 kill",
    "update": "node scripts/update.js",
